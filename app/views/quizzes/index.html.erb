<div class ="booyah-box col-10 offset-1">
  <h2>Quiz</h2>
  <div class="col-12">
  <% @questions.each do |question| %>
    <br />
    <h5>Question #  <%= question.id %></h5>
    <br />
    <b>Question:</b>  <%= question.question_name %>
    <br />
    <b>Description:</b>  <%= question.description %>
    <br />
  <% end %>
    </div>
  <br />
  <br />
  <%= link_to 'Add Question', new_question_path, class: 'btn btn-success' %>
</div>